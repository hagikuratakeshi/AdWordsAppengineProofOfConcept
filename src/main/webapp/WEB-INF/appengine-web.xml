<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>takeshi-dfp-playground</application>
  <version>1</version>
  <threadsafe>false</threadsafe>

  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
    <!-- If you do not have a client ID or secret, please create one in the API console:
      https://code.google.com/apis/console#access -->
    <property name="adwords.appengine.clientId" value="INSERT_YOUR_CLIENT_ID" />
    <property name="adwords.appengine.clientSecret" value="INSERT_YOUR_CLIENT_SECRET" />
    <property name="adwords.clientCustomerId" value="INSERT_YOUR_CLIENT_CUSTOMER_ID" />
    <property name="adwords.developerToken" value="INSERT_YOUR_DEVELOPER_TOKEN" />
  </system-properties>
</appengine-web-app>

